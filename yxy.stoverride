name: '🎓 uLearning Filter'
desc: 'uLearning学习平台行为追踪拦截与缓存数据过滤'
author: 'YourName'
homepage: 'https://your-repo.io'
icon: 'https://example.com/icon.png'

http:
  mitm:
    - "utestapi.ulearning.cn"
    - "apps.ulearning.cn"
  script:
    - match: ^https?://utestapi\.ulearning\.cn/exams/(setBehaviorTrace|saveLocalCache)
      name: ulearning-filter-request
      type: request
      require-body: true
      timeout: 10
    - match: ^https?://apps\.ulearning\.cn/exam/saveExamLimit\.do
      name: ulearning-filter-request
      type: request
      require-body: true
      timeout: 10

script-providers:
  ulearning-filter-request:
    url: https://raw.githubusercontent.com/FlyRenxing/yxyscript/refs/heads/main/ulearning-filter.js
    interval: 86400
